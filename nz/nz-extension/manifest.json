{
  "name": "nz-extension",
  "version": "1.0",
  "manifest_version": 3,
  "action": {},
  "description": "",
  "permissions": [
    "activeTab",
    "storage",
    "tabs",
    "webRequest"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "https://onlineservices.immigration.govt.nz/*"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "inject.js"
      ],
      "matches": [
        "https://onlineservices.immigration.govt.nz/*"
      ],
      "use_dynamic_url": true
    }
  ]
}